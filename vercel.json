{
  "version": 2,
  "builds": [
    {
      "src": "api/pdf.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/pdf",
      "dest": "/api/pdf.js"
    }
  ],
  "functions": {
    "api/pdf.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}